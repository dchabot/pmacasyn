record(ao, "$(d1)Scale") {
  field(FLNK, "$(assy)ScaleFanD.PROC PP MS")
  field(DOL, "1.0e-04")
  field(PREC, "6")
}

record(ao, "$(d1)Offset") {
  field(FLNK, "$(assy)ScaleFanD.PROC PP MS")
  field(PREC, "5")
}

record(ao, "$(d2)Scale") {
  field(FLNK, "$(assy)ScaleFanD.PROC PP MS")
  field(DOL, "1.0e-04")
  field(PREC, "6")
}

record(ao, "$(d2)Offset") {
  field(FLNK, "$(assy)ScaleFanD.PROC PP MS")
  field(PREC, "5")
}

record(bo,"$(assy)Invert") {
  field(PINI,"YES")
  field(FLNK,"$(assy)ScaleFanD.PROC PP MS")
  field(DOL,"0")
  field(ZNAM,"+1")
  field(ONAM,"-1")
}

record(longout, "$(assy)AccelTime") {
  field(FLNK, "$(assy)ChangedFLNK.PROC PP MS")
  field(DOL, "1000")
}

record(longout, "$(assy)AccelCurve") {
  field(FLNK, "$(assy)ChangedFLNK.PROC PP MS")
  field(DOL, "500")
}
