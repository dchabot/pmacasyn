record(bi, "$(x1)AmpEna") {
  field(DESC, "Amplifier Enable")
  field(ZSV, "MAJOR")
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")
# field(PINI, "YES")
  field(INP, "$(x1)AmpEnaCALC.VAL PP MS")
} 

