record(calc, "$(assy)$(val)SDISm") {
  field(CALC, "A|B")
  field(INPA, "$(assy)$(val)TsubAxs.PACT NPP MS")
  field(INPB, "$(assy)$(val)TsubDrv.PACT NPP MS")
}

record(calc, "$(assy)$(val)SDISd") {
  field(CALC, "A|B")
  field(INPA, "$(assy)$(val)TsubAxs.PACT NPP MS")
  field(INPB, "$(assy)$(val)TsubMtr.PACT NPP MS")
}

record(calc, "$(assy)$(val)SDISx") {
