file "$(TPMAC)/pmacDb/AsciiPcs.db"
{
	pattern { pcs, C, x }
#				pcs =   1   2   3   4   5   6   7   8
#				  x =   1   2   3   4   5   6   7   8
#
# In these files "pcs" is the coordinate system number.
# pcs == _p_mac _c_oordinate _s_ystem
#
# pmac20:
        { 23i:CCD:Lp:, 0,  1 }
        { 23i:CCD:St:, 0,  2 }
        { 23i:GO:Om:,  0,  3 }
        { 23i:GO:Kp:,  0,  4 }
        { 23i:GO:Cr:,  0,  5 }
 	{ 23i:GO:St:,  0,  6 }
 	{ 23i:JET:Ps:, 0,  7 }
 	{ 23i:BD:Dh:,  0,  8 }
 	{ 23i:BD:Dv:,  0,  9 }
        { 23i:GS:St:,  0, 10 }
        { 23i:GS:Av:,  0, 11 }
  	{ 23i:GS:Ah:,  0, 12 }
  	{ 23i:BS:Ps:,  0, 13 }
  	{ 23i:TV:V1:,  0, 14 }
  	{ 23i:TV:V2:,  0, 15 }
  	{ 23i:SH:Ps:,  0, 16 }
}
