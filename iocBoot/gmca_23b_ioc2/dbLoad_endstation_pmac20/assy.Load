file "$(TPMAC)/softDb/Load05X3.db"
{
	pattern { assy, m1, C, pcs }
#                              pcs =   1   2   3   4   5   6   7   8
#
# In these files "pcs" is the coordinate system number.
# pcs == _p_mac _c_oordinate _s_ystem
#
# pmac20:
        { 23b:CCD:LpB:, 23b:CCD:ml:,  0,  1 }
        { 23b:GO:Om:,   23b:GO:mom:,  0,  3 }
        { 23b:JET:Ps:,  23b:JET:mp:,  0,  7 }
        { 23b:BD:DhB:,  23b:BD:mhd:,  0,  8 }
        { 23b:TV:V1:,   23b:TV:mz1:,  0, 14 }
        { 23b:TV:V2:,   23b:TV:mz2:,  0, 15 }
        { 23b:SH:Ps:,   23b:SH:mp:,   0, 16 }
}
